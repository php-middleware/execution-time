{
    "name": "php-middleware/response-time",
    "description": "PSR-7 middleware measuring PHP execution time",
    "type": "library",
    "keywords": [
        "middleware",
        "psr",
        "psr-7",
        "response time"
    ],
    "require": {
        "php": ">=5.5",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.6"
    },
    "autoload": {
        "psr-4": {
            "PhpMiddleware\\ResponseTime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpMiddlewareTest\\ResponseTime\\": "test/"
        }
    }
}
